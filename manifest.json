{
  "name": "NewTabFlow",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A simple Chrome new tab extension",
  "homepage_url": "https://github.com/DCKT/new-tab-flow",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "build/background.html",
    "persistent": true
  },
  "options_page": "build/options.html",
  "chrome_url_overrides": {
    "newtab": "build/override.html"
  }
}
